{
    "name": "leantime/fixtures",
    "description": "Loads fixtures into database",
    "version": "0.1",
    "screenshot": "",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        }
    ],
    "type": "leantime-plugin",
    "license": "MIT",
    "homepage": "https://github.com/rimi-itk/leantime-plugin-fixtures",
    "require": {
        "leantime/leantime": "^3.0"
    },
    "repositories": {
        "leantime/leantime": {
            "type": "vcs",
            "url": "https://github.com/Leantime/leantime"
        }
    },
    "config": {
        "allow-plugins": {
            "metasyntactical/composer-plugin-license-check": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.9",
        "friendsofphp/php-cs-fixer": "^3.51",
        "phpcsstandards/phpcsextra": "^1.2",
        "zebra-north/phpcs-short-types": "^1.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "coding-standards-check/phpcs": [
            "phpcs --ignore='*/vendor/*' ."
        ],
        "coding-standards-check": [
            "@coding-standards-check/phpcs"
        ],
        "coding-standards-apply/phpcbf": [
            "phpcbf --ignore='*/vendor/*' ."
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/phpcbf"
        ],
        "code-analysis/phpstan": [
            "phpstan analyse"
        ],
        "code-analysis": [
            "@code-analysis/phpstan"
        ]
    }
}
